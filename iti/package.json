{
  "name": "iti",
  "version": "0.7.0",
  "description": "~1kB Dependency Injection Library for Typescript and React with a unique async flow support",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.mts",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "source": "src/index.ts",
  "publishConfig": {
    "source": "./src/index.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "make build",
    "stryker": "stryker",
    "stryker:run": "stryker run",
    "test": "vitest && yarn tsd",
    "tsd": "tsd tsd_project",
    "v": "vitest",
    "vitest:types": "vitest --typecheck"
  },
  "dependencies": {
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.1.2",
    "@stryker-mutator/jest-runner": "^6.1.2",
    "@stryker-mutator/typescript-checker": "^6.1.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.15",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "nodemon": "^2.0.19",
    "stryker-cli": "^1.0.2",
    "ts-jest": "^29.2.5",
    "tsd": "^0.22.0",
    "typescript": "^4.7.4",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "authors": [
    "Nick Olszanski <nick@dinosaurs-with-jetpacks.com>"
  ],
  "repository": "molszanski/iti",
  "keywords": [
    "ioc",
    "di",
    "inversion of control",
    "dependency injection",
    "dependency inversion",
    "inversion of control container",
    "container",
    "javascript",
    "typescript",
    "type-safe"
  ],
  "homepage": "https://itijs.org",
  "// CONFIGS: ": "Package configs",
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "arrowParens": "always",
    "trailingComma": "all"
  }
}
